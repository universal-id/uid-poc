<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="StartView"
             x:Class="ClinetOnline.Views.StartView"
             BackgroundColor="{DynamicResource SecondaryColor}"
             BindingContext="{Binding CurrentViewModel}">
    <ScrollView>
        <Grid>
            <Button Text="Start using the app"  Margin="100"  Command="{Binding StartCommand}"/>
        </Grid>
    </ScrollView>

</ContentPage>